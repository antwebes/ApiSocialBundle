{% if  (is_granted('IS_AUTHENTICATED_FULLY') or  is_granted('IS_AUTHENTICATED_REMEMBERED')) and channel.owner.id != app.user.id %}
<script>
	$(document).ready(function(){
		$.ajax({
			type: 'PUT',
			url: '{{ api_endpoint }}/api/channels/{{ channel.id }}/visits?access_token={{ app.user.accessToken }}'
		});
	});
</script>
{% endif %}