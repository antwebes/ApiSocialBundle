       
     <div class="col-xs-12 center">
		<span class="profile-picture">
		<div style="position: relative; left: 0; top: 0;">
			{% if user.profilePhoto %}
				<img style="position: relative; top: 0; left: 0;" class="editable img-responsive" alt="{{ user.username | capitalize }}" id="avatar2" src="{{ user | userProfilePhotoUrl("medium") }}">
			{% else %}
				{% image '@ApiSocialBundle/Resources/public/images/no-photo-medium.png'  output="images/no-photo-medium.png" %}
					<img style="position: relative; top: 0; left: 0;" class="editable img-responsive" alt="{{ user.username | capitalize }}" id="avatar2" src="{{ asset_url }}">
				{% endimage %}
			{% endif %}
			{# begin badge account validated #}
			{% if user.enabled %}
				<i style="position: absolute; top: 80%; left: 80%;" class="fa fa-check-circle-o fa-3x blue" data-toggle="tooltip" data-placement="right" data-behat="account-verified" title="Account verified"></i>
			{% endif %}
			{# end badge account validated #}
		</div>
		
		</span>

		<div class="space space-4"></div>

